{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "eslint.enable": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript"
  ],
  "files.autoSave": "onFocusChange",
  "editor.rulers": [
    120
  ],
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#3D3E40"
  },
}